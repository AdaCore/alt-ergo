(set-option :produce-models true)
(set-logic ALL)
(declare-datatype Data (
  (cons1 (d1 Real) (d2 Real))
  (cons2)
  (cons3 (d3 Real))
))
(declare-const a Data)
(declare-const b Data)
(declare-const x Real)
(declare-const y Real)
(assert ((_ is cons1) a))
(assert ((_ is cons3) b))
(assert (= (d1 a) x))
(assert (= (d3 b) y))
(assert (= (d1 a) (- 2 (d3 b))))
(check-sat)
(get-model)
